<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Email Address</th>
        <th scope="col">Job Title</th>
      </tr>
    </thead>
    <tbody>
      <% @people.each do |person| %>
        <tr>
          <td><%= person["display_name"] %></td>
          <td><%= person["email_address"]%></td>
          <td><%= person["title"]%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>